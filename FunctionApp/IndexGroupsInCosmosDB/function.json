{
  "bindings": [
    {
      "name": "ActivityInput",
      "type": "activityTrigger",
      "direction": "in"
    },
    {
      "name": "groupsRawOut",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "EntraData",
      "containerName": "groups_raw",
      "connection": "CosmosDbConnectionString",
      "createIfNotExists": false,
      "partitionKey": "/objectId"
    },
    {
      "name": "groupChangesOut",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "EntraData",
      "containerName": "group_changes",
      "connection": "CosmosDbConnectionString",
      "createIfNotExists": false,
      "partitionKey": "/snapshotId"
    },
    {
      "name": "snapshotsOut",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "EntraData",
      "containerName": "snapshots",
      "connection": "CosmosDbConnectionString",
      "createIfNotExists": false,
      "partitionKey": "/id"
    },
    {
      "name": "groupsRawIn",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "EntraData",
      "containerName": "groups_raw",
      "connection": "CosmosDbConnectionString",
      "sqlQuery": "SELECT c.objectId, c.displayName, c.classification, c.description, c.groupTypes, c.mailEnabled, c.membershipRule, c.securityEnabled, c.isAssignableToRole, c.visibility, c.onPremisesSyncEnabled, c.mail, c.deleted FROM c"
    }
  ]
}
