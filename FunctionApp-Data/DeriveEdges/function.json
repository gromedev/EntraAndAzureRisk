{
  "bindings": [
    {
      "name": "ActivityInput",
      "type": "activityTrigger",
      "direction": "in"
    },
    {
      "name": "edgesIn",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "EntraData",
      "containerName": "edges",
      "connection": "CosmosDbConnectionString",
      "sqlQuery": "SELECT * FROM c WHERE (NOT IS_DEFINED(c.deleted) OR c.deleted = false)"
    },
    {
      "name": "edgesOut",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "EntraData",
      "containerName": "edges",
      "connection": "CosmosDbConnectionString",
      "createIfNotExists": false,
      "partitionKey": "/edgeType"
    }
  ]
}
